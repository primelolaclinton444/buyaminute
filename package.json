{
  "name": "buyaminute",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/vercel-build.js",
    "start": "next start",
    "test": "npm run test:build && node --require ./tests/register-test-env.js --test --test-concurrency 1 .test-dist/tests/**/*.test.js",
    "test:build": "npm run prisma:validate && prisma generate && tsc -p tsconfig.test.json",
    "postinstall": "prisma generate",
    "prisma:validate": "DATABASE_URL=\"${DATABASE_URL:-file:./dev.db}\" prisma validate",
    "prisma:migrate": "DATABASE_URL=\"${DATABASE_URL:-file:./dev.db}\" prisma migrate deploy",
    "send:deposit": "node --loader ts-node/esm scripts/send-deposit-intake.ts"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@prisma/client": "^5.7.0",
    "tronweb": "^6.0.0",
    "livekit-server-sdk": "^2.7.0",
    "@upstash/redis": "^1.34.0",
    "@upstash/ratelimit": "^2.0.5"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0"
  }
}
